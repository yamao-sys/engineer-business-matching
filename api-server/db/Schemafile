# encoding: utf-8

# -*- mode: ruby -*-
# vi: set ft=ruby :

create_table "companies", charset: "utf8mb4", collation: "utf8mb4_0900_ai_ci", force: :cascade do |t|
  t.bigint "industry_id"
  t.string "name", null: false
  t.string "email", null: false
  t.text "password_digest"
  t.text "final_tax_return_data"
  t.text "logo_data"
  t.text "address"
  t.text "site_url"
  t.integer "employee_count"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.index [ "email" ], name: "index_companies_on_email"
end

create_table "engineers", charset: "utf8mb4", collation: "utf8mb4_0900_ai_ci", force: :cascade do |t|
  t.string "first_name", null: false
  t.string "last_name", null: false
  t.string "email", null: false
  t.text "password_digest"
  t.date "birthday", null: false
  t.text "front_identification_data"
  t.text "back_identification_data"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.index [ "email" ], name: "index_engineers_on_email"
end

create_table "industries", charset: "utf8mb4", collation: "utf8mb4_0900_ai_ci", force: :cascade do |t|
  t.string "name", null: false
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end
